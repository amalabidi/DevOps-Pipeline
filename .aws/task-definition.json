{
    "family": "Amal-task-def-pipeline",
    "containerDefinitions": [{
        "name": "Amal-devops-MyApp",
        "image": "17895/my-app",
        "portMappings": [{
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp"
        }]
    }],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256"
}